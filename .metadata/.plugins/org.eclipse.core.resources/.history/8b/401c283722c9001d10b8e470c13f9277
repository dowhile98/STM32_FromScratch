/*Globla variables ----------------------------------*/

/**
 * @brief inicializar la instancia para el manejo del dispositivo por comandos AT
 */
void AT_Device_Init(AT_CMD_Parse_t *dev){
	//inicializar el buffer
	lwrb_init(&(dev->buff), dev->buff_data, 1024);
	//comando activo
	dev->ACTIV_CMD = CMD_NONE;
	return;
}
