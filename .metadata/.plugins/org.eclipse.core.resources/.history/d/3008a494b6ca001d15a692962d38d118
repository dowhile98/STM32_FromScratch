#ifndef COMPONENTS_BUTTONV2_H_
#define COMPONENTS_BUTTONV2_H_

/*Includes -------------------------------------------------- */
#include "stm32f4xx.h"
#include <stdbool.h>

/*Typedef ---------------------------------------------------*/
typedef struct{
	GPIO_TypeDef *GPIO;
	uint8_t pin;
	uint8_t state;
	uint32_t ticks;
	uint8_t pulseCount;
};

/*Function prototype ----------------------------------------*/




#endif /* COMPONENTS_BUTTONV2_H_ */
