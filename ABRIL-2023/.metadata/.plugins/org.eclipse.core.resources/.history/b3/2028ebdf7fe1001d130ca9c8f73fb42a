#include <stdint.h>
#include <stm32f4xx.h>



int main(void)
{

	RCC_TypeDef *PTRRCC = (RCC_TypeDef*)(RCC_BASE);
	GPIO_TypeDef *PORTD = (GPIO_TypeDef*)(GPIOD_BASE);

	//PD12->salida de proposito general
	//Habilitar el reloj
	PTRRCC->AHB1ENR |= RCC_AHB1ENR_GPIODEN;

	PORTD->MODER &=~ GPIO_MODER_MODE12;
	PORTD->MODER |= GPIO_MODER_MODE12_0;//salida de proposito general

    /* Loop forever */
	for(;;){
		PORTD->ODR |= GPIO_ODR_OD12;
		PORTD->ODR &=~GPIO_ODR_OD12;
	}
}
