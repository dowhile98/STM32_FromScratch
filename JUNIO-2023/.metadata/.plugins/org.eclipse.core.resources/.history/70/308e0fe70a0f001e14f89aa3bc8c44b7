#ifndef BSP_DISPLAY_H_
#define BSP_DISPLAY_H_

/*Includes ----------------------------------------------------*/
#include <stdint.h>
/*Typedef -----------------------------------------------------*/
typedef enum{
	DIGIT1,
	DIGIT2
}Digit_t;

typedef struct{
	uint8_t frame[2];
}Display_t;
/*Function prototype ------------------------------------------*/
void display_write(uint8_t data, uint8_t digit, uint8_t dp);


#endif /* BSP_DISPLAY_H_ */
