/*Includes ------------------------------------------------------*/
#include "SysTick.h"


void SysTick_Init(uint32_t ticks){
	SysTick->CTRL = 0;

	SysTick->LOAD = ticks - 1;

	SysTick->VAL = 0;

	SysTick->CTRL = SysTick_CTRL_ENABLE_Msk | SysTick_CTRL_CLKSOURCE_Msk;

	return;
}

