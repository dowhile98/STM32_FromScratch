#ifndef BSP_UART_H_
#define BSP_UART_H_

/*Includes ---------------------------------------------------*/
#include "stm32f4xx.h"

/*Typedefs --------------------------------------------------*/
typedef struct{
	uint8_t stopBit;
	uint8_t parity;
	uint8_t over;
	uint32_t baud;
}UART_ConfigTypedef_t;
/*Function prototype ----------------------------------------*/

#endif /* BSP_UART_H_ */
